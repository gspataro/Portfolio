{
    "website": {
        "name": "Giuseppe Spataro",
        "description": "Giuseppe Spataro, pianista e sviluppatore web siciliano",
        "author": "Giuseppe Spataro",
        "keywords": "pianista, sviluppatore web, sicilia",
        "url": "http://gspataro.test"
    },
    "languages": ["it", "en"],
    "default_language": "it",
    "items": {
        "project": {
            "template": "project",
            "output": "/project",
            "builder": "post",
            "contents": {
                "project": "markdown:projects/*.md"
            }
        },
        "projects": {
            "template": "projects",
            "output": "/projects.html",
            "contents": {
                "projects": "markdown:projects/*.md"
            }
        },
        "index": {
            "template": "index",
            "output": "/index.html",
            "contents": {
                "projects": "markdown:projects/cli.md;projects/portfolio.md;projects/dependencyinjection.md"
            }
        },
        "colors": {
            "template": "colors",
            "output": "/colors.html"
        },
        "typography": {
            "template": "typography",
            "output": "/typography.html"
        }
    },
    "exclude_assets": [
        "ui.css"
    ]
}
