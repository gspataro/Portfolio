{
    "website": {
        "name": "Giuseppe Spataro",
        "description": "Giuseppe Spataro, pianista e sviluppatore web siciliano",
        "author": "Giuseppe Spataro",
        "keywords": "pianista, sviluppatore web, sicilia",
        "url": "http://gspataro.test"
    },
    "languages": ["it", "en"],
    "default_language": "it",
    "contents": {
        "page": {
            "data": "markdown:pages/*.md",
            "template": "page",
            "output": "/"
        },
        "project": {
            "data": "markdown:projects/*.md",
            "template": "project",
            "output": "/project",
            "archive": {
                "template": "projects",
                "slug": "projects"
            }
        }
    },
    "exclude_assets": [
        "ui.css"
    ]
}
