{
    "website": {
        "name": "Giuseppe Spataro",
        "description": "Giuseppe Spataro, pianista e sviluppatore web siciliano",
        "author": "Giuseppe Spataro",
        "keywords": "pianista, sviluppatore web, sicilia",
        "url": "http://gspataro.test"
    },
    "languages": ["it", "en"],
    "default_language": "it",
    "contents": {
        "projects": "markdown:projects/*.md"
    },
    "schemas": {
        "index": {
            "contents": [
                "projects"
            ],
            "template": "index",
            "builder": "simple",
            "slug": "/index",
            "generate": "once"
        },
        "project": {
            "contents": [
                "projects"
            ],
            "template": "project",
            "builder": "post",
            "slug": "/project",
            "generate": "loop:projects"
        }
    },
    "exclude_assets": [
        "ui.css"
    ]
}
