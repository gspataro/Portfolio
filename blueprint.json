{
    "website": {
        "url": "http://gspataro.test"
    },
    "languages": ["it", "en"],
    "default_language": "it",
    "items": [
        {
            "template": "project",
            "output": "/project",
            "group": "project",
            "builder": "projects",
            "contents": {
                "projects": "markdown:projects/*.md"
            }
        },
        {
            "template": "index",
            "output": "/index.html",
            "contents": {
                "projects": "markdown:projects/cli.md;projects/portfolio.md;projects/dependencyinjection.md"
            }
        },
        {
            "template": "colors",
            "output": "/colors.html"
        },
        {
            "template": "typography",
            "output": "/typography.html"
        }
    ],
    "exclude_assets": [
        "ui.css"
    ]
}
